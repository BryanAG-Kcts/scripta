{
  "manifest_version": 3,
  "name": "Nombre de la extensión",
  "version": "1.0",
  "description": "Descripción de la extensión",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "48": "./medium-wawe.svg"
    }
  },
  "icons": {
    "48": "./medium-wawe.svg"
  },
  "permissions": [
    "scripting",
    "activeTab",
    "storage",
    "tabs"
  ],
  "content_scripts": [
  {
    "matches": ["<all_urls>"],
    "js": ["./assets/content.js"],
    "run_at": "document_idle"
  }
],
"background": {
  "service_worker": "background.js"
}
}
